@inject ISystemSettingInterface _sysSettings;
@inject ICookiesService _cookies;
@{
    var settingRes = await _sysSettings.ReadAsync();
    var settings = settingRes.Data;
    var userData = _cookies.Read();
    var firstChar = userData.FirstName?.ElementAtOrDefault(0);
    var firstDayOfWeek = Helpers.FirstDayOfWeek().ToString("dd-MMM-yyyy");
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@settings.Name | @ViewData["Title"]</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="~/plugins/fontawesome-free/css/all.min.css">
    <link rel="stylesheet" href="~/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
    <link rel="stylesheet" href="~/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <link rel="stylesheet" href="~/plugins/jqvmap/jqvmap.min.css">
    <link rel="stylesheet" href="~/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
    <link rel="stylesheet" href="~/plugins/daterangepicker/daterangepicker.css">
    <link rel="stylesheet" href="~/plugins/summernote/summernote-bs4.css">
    <link rel="stylesheet" href="~/plugins/select2/css/select2.min.css">
    <link rel="stylesheet" href="~/plugins/select2-bootstrap4-theme/select2-bootstrap4.min.css">
    <link rel="stylesheet" href="~/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="~/plugins/datatables-responsive/css/responsive.bootstrap4.min.css">
    <link rel="stylesheet" href="~/plugins/datatables-buttons/css/buttons.bootstrap4.min.css">
    <link rel="stylesheet" href="~/lib/google-fonts/SourceSansPro.css">
    <link href="~/lib/toastr/toastr.min.css" rel="stylesheet">
    <link href="~/css/site.css" rel="stylesheet" asp-append-version="true">
    <link rel="stylesheet" href="~/plugins/summernote/summernote-bs4.min.css">
    <link rel="stylesheet" href="~/dist/css/adminlte.min.css">
</head>

<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a asp-controller="Dashboard" asp-action="Home" class="nav-link">Home</a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a asp-controller="Dashboard" asp-action="Contact" class="nav-link">Contact</a>
                </li>
            </ul>
            <form method="get" class="form-inline ml-3" autocomplete="off" asp-action="ViewProduct"
                  asp-controller="Products">
                <input type="hidden" name="Id" value="@userData.InstanceId" />
                <div class="input-group input-group-sm">
                    <input class="form-control form-control-navbar" name="Code" value="@ViewData["ProductCode"]"
                           placeholder="Search code in @userData.InstanceName" aria-label="Search">
                    <div class="input-group-append">
                        <button class="btn btn-navbar" type="submit">
                            <i class="fas fa-search"></i>
                        </button>
                    </div>
                </div>
            </form>

            <ul class="navbar-nav ml-auto">
                <li class="nav-item d-none d-sm-inline-block">
                    <a asp-controller="Dashboard" asp-action="Index" class="nav-link">
                        <i class="fa fa-home"></i>&nbsp;
                        @userData.ClientName
                        @if (User.IsInRole(Role.Clerk))
                        {
                            <span>(@userData.InstanceName)</span>
                        }
                    </a>
                </li>
                <li class="dropdown user user-menu nav-item  d-sm-inline-block">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        <div class="image">
                            @if (string.IsNullOrEmpty(userData.ImagePath))
                            {
                                <span class="i-circle">@firstChar</span>
                            }
                            else
                            {
                                <img src="~/@userData.ImagePath" width="60" height="60" class="img-circle elevation-2"
                                 alt="@userData.FirstName">
                            }
                        </div>
                        <span class="hidden-xs">@userData.FullName</span>
                    </a>
                    <ul class="dropdown-menu">
                        <li class="user-header">
                            <div class="image">
                                @if (string.IsNullOrEmpty(userData.ImagePath))
                                {
                                    <span class="i-circle">@firstChar</span>
                                }
                                else
                                {
                                    <img src="~/@userData.ImagePath" width="160" height="160" class="img-circle elevation-2"
                                     alt="@userData.FirstName">
                                }
                            </div>
                            <p>
                                @userData.FullName - @userData.Role
                                <small>Added On : @userData.DateCreated</small>
                            </p>
                        </li>
                        <li class="card-footer form-inline">
                            <div>
                                <a asp-controller="Users" asp-action="Edit" asp-route-id="@userData.UserId"
                                   class="btn btn-primary btn-flat">Profile</a>
                            </div>
                            <span>&nbsp;</span>
                            <div class="mr-0">
                                <a asp-controller="Home" asp-action="Logout" class="btn btn-danger btn-flat">
                                    Sign out
                                </a>
                            </div>
                        </li>
                    </ul>
                </li>
            </ul>
        </nav>

        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <a asp-controller="Dashboard" asp-action="Index" class="brand-link">
                @if (string.IsNullOrEmpty(userData.ClientLogoPath))
                {
                    <span class="i-circle">@userData.ClientName?.ElementAtOrDefault(0)</span>
                }
                else
                {
                    <img src="~/@userData.ClientLogoPath" alt="@userData.ClientCode"
                     class="brand-image img-circle elevation-3" style="opacity: .8">
                }
                <span class="brand-text font-weight-light">@userData.ClientCode</span>
            </a>
            <div class="sidebar">
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu"
                        data-accordion="false">
                        <li class="nav-item">
                            <a asp-controller="Dashboard" asp-action="Index" class="nav-link" id="MenuDashboard">
                                <i class="nav-icon fas fa-tachometer-alt"></i>
                                <p>
                                    Dashboard
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview" id="MenuPOS">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fa fa-calculator"></i>
                                <p>
                                    Point Of Sale
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a asp-controller="PointOfSale" asp-action="Index" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Sell Now</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="PointOfSale" asp-action="ReceivePayment" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Receive Payment</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="PointOfSale" asp-action="Receipts"
                                       asp-route-dtFrom="@firstDayOfWeek"
                                       asp-route-dtTo="@DateTime.Now.ToString("dd-MMM-yyyy")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Sale Receipts</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="PointOfSale" asp-action="Invoices"
                                       asp-route-dtFrom="@firstDayOfWeek"
                                       asp-route-dtTo="@DateTime.Now.ToString("dd-MMM-yyyy")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Invoices</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="PointOfSale" asp-action="GeneralLedgers"
                                       asp-route-dtFrom="@DateTime.Now.ToString("dd-MMM-yyyy")"
                                       asp-route-dtTo="@DateTime.Now.ToString("dd-MMM-yyyy")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>General Ledger</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="PointOfSale" asp-action="Expenses" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Expenses</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="Customers" asp-action="ByClientId" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Customers</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="Settings" asp-action="UploadExcel"
                                       asp-route-id="@UploadExelOption.Customers" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Upload Customers</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="#" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>
                                            Orders
                                            <i class="right fas fa-angle-left"></i>
                                        </p>
                                    </a>
                                    <ul class="nav nav-treeview">
                                        <li class="nav-item">
                                            <a asp-controller="Orders" asp-action="Index"
                                               asp-route-dtFrom="@firstDayOfWeek"
                                               asp-route-dtTo="@DateTime.Now.ToString("dd-MMM-yyyy")" class="nav-link">
                                                <i class="far fa-circle nav-icon"></i>
                                                <p>View All</p>
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a asp-controller="Orders" asp-action="PlaceOrder" asp-route-id=""
                                               class="nav-link">
                                                <i class="far fa-circle nav-icon"></i>
                                                <p>Place Order</p>
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                                <li class="nav-item" style="display:none">
                                    <a class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Return Items</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        @if (!userData.Role.Equals(Role.Clerk))
                        {
                            <li class="nav-item has-treeview" id="MenuHR">
                                <a href="#" class="nav-link">
                                    <i class="nav-icon fas fa-users"></i>
                                    <p>
                                        Human Resource
                                        <i class="right fas fa-angle-left"></i>
                                    </p>
                                </a>
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a asp-controller="Users" asp-action="All" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Employees</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-controller="HumanResource" asp-action="EmployeeSalaries" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Employee Salaries</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-controller="HumanResource" asp-action="LeaveApplications"
                                       asp-route-dtFrom="@Helpers.firstDayOfYear.ToString("dd-MMM-yyyy")"
                                       class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Leave Applications</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-controller="HumanResource" asp-action="LeaveCategories" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Leave Categories</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-controller="HumanResource" asp-action="Banks" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Banks</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-controller="HumanResource" asp-action="MonthlyPayments"
                                       asp-route-year="@DateTime.Now.Year" asp-route-month="@DateTime.Now.Month"
                                       class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Monthly Payments</p>
                                        </a>
                                    </li>
                                </ul>
                            </li>
                        }
                        @if (User.IsInRole(Role.SuperAdmin))
                        {
                            <li class="nav-item has-treeview" id="MenuClients">
                                <a href="#" class="nav-link">
                                    <i class="nav-icon fas fa-list"></i>
                                    <p>
                                        Clients
                                        <i class="right fas fa-angle-left"></i>
                                    </p>
                                </a>
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a asp-controller="Clients" asp-action="All" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>View All</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-controller="Clients" asp-action="Edit" asp-route-id="" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Add New</p>
                                        </a>
                                    </li>
                                    <li class="nav-item" style="display:none">
                                        <a asp-controller="Clients" asp-action="Summary" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Summary</p>
                                        </a>
                                    </li>
                                </ul>
                            </li>
                        }
                        @if (User.IsInRole(Role.Manager) || User.IsInRole(Role.SuperAdmin))
                        {
                            <li class="nav-item has-treeview" id="MenuInstances">
                                <a href="#" class="nav-link">
                                    <i class="nav-icon fas fa-table"></i>
                                    <p>
                                        Instances
                                        <i class="right fas fa-angle-left"></i>
                                    </p>
                                </a>
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a asp-controller="ClientInstances" asp-action="All" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>View All</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-controller="ClientInstances" asp-action="Edit" asp-route-id=""
                                       class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Add New</p>
                                        </a>
                                    </li>
                                    <li class="nav-item" style="display:none">
                                        <a asp-controller="ClientInstances" asp-action="All" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Summary</p>
                                        </a>
                                    </li>
                                </ul>
                            </li>
                        }
                        <li class="nav-item has-treeview" id="MenuProducts">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-th"></i>
                                <p>
                                    Stock Control
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a asp-controller="Products" asp-action="Index" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Products</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="Products" asp-action="ProductPrice" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Product Price</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="Products" asp-action="PurchaseOrders"
                                       asp-route-dtFrom="@firstDayOfWeek"
                                       asp-route-dtTo="@DateTime.Now.ToString("dd-MMM-yyyy")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Purchase Orders</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="Products" asp-action="ReceivedGoods"
                                       asp-route-dtFrom="@firstDayOfWeek"
                                       asp-route-dtTo="@DateTime.Now.ToString("dd-MMM-yyyy")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Received Goods</p>
                                    </a>
                                </li>
                                @if (User.IsInRole(Role.SuperAdmin) || User.IsInRole(Role.Manager))
                                {
                                    <li class="nav-item">
                                        <a asp-controller="Products" asp-action="ProductStockAdjustment" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Stock Adjustment</p>
                                        </a>
                                    </li>
                                }
                                <li class="nav-item">
                                    <a asp-controller="Suppliers" asp-action="ByClientId" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Suppliers</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="Settings" asp-action="UploadExcel"
                                       asp-route-id="@UploadExelOption.Products" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Upload Products</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item has-treeview" id="MenuMasterData">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fa fa-database"></i>
                                <p>
                                    Master Data
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a asp-controller="MasterData" asp-action="ProductCategories" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Product Categories</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="MasterData" asp-action="UnitOfMeasures" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Unit of Measures</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="MasterData" asp-action="ExpenseTypes" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Expense Types</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="MasterData" asp-action="PaymentModes" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Payment Modes</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="MasterData" asp-action="TaxTypes" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Tax Types</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        @if (User.IsInRole(Role.SuperAdmin) || User.IsInRole(Role.Manager))
                        {
                            <li class="nav-item has-treeview" id="MenuReports">
                                <a href="#" class="nav-link">
                                    <i class="nav-icon fa fa-chart-pie"></i>
                                    <p>
                                        Reports
                                        <i class="right fas fa-angle-left"></i>
                                    </p>
                                </a>
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a asp-controller="Reports" asp-action="SalesReport"
                                       asp-route-dtFrom="@firstDayOfWeek"
                                       asp-route-dtTo="@DateTime.Now.ToString("dd-MMM-yyyy")" asp-route-option="Weekly"
                                       class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Weekly Report</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-controller="Reports" asp-action="SalesReport"
                                       asp-route-dtFrom="@Helpers.firstDayOfMonth.ToString("dd-MMM-yyyy")"
                                       asp-route-dtTo="@Helpers.lastDayOfMonth.ToString("dd-MMM-yyyy")"
                                       asp-route-option="Monthly" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Monthly Report</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-controller="Reports" asp-action="CustomerBalances" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Customer Balances</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-controller="Settings" asp-action="SendTestEmail" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Send Email</p>
                                        </a>
                                    </li>
                                </ul>
                            </li>
                        }
                        @if (User.IsInRole(Role.SuperAdmin))
                        {
                            <li class="nav-item has-treeview" id="MenuSettings">
                                <a href="#" class="nav-link">
                                    <i class="nav-icon fas fa-cog"></i>
                                    <p>
                                        Settings
                                        <i class="right fas fa-angle-left"></i>
                                    </p>
                                </a>
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a asp-controller="Settings" asp-action="EmailSettings" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Email Settings</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-controller="Settings" asp-action="SendTestEmail" asp-route-test="True"
                                       class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Test Email</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-controller="Settings" asp-action="GlobalSystem" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>System Settings</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a asp-controller="Settings" asp-action="TermsAndPrivacy" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Home,&nbsp;Terms &amp; Privacy</p>
                                        </a>
                                    </li>

                                </ul>
                            </li>
                        }
                        <li class="nav-item">
                            <a asp-controller="Home" asp-action="Logout" class="nav-link">
                                <i class="nav-icon fas fa-lock"></i>
                                <p>
                                    Logout
                                </p>
                            </a>
                        </li>
                    </ul>
                </nav>

            </div>
        </aside>

        <div class="content-wrapper">
            <div class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1 class="m-0 text-dark">@ViewData["Title"]</h1>
                        </div>
                        <div class="col-sm-6">
                            <ol class="breadcrumb float-sm-right">
                                <li class="breadcrumb-item">
                                    <a asp-controller="Dashboard" asp-action="Index">Home</a>
                                </li>
                                <li class="breadcrumb-item active">@ViewData["Title"]</li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
            <section class="content">
                <div class="container-fluid">
                    <main role="main" class="pb-3">
                        @{
                            var licMsg = TempData["LicenceMsg"]?.ToString() ?? "";
                            if (!string.IsNullOrEmpty(licMsg))
                            {
                                <p class="alert alert-info text-center"> <i class="fa fa-exclamation-circle"></i> @licMsg</p>
                            }
                        }
                        @RenderBody()
                    </main>
                </div>
            </section>
        </div>
        <a asp-action="Index" asp-controller="PointOfSale" class="float d-print-none">
            <i class="fa fa-shopping-cart my-float"></i>
        </a>
        <footer class="main-footer d-print-none">
            <strong>
